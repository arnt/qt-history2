!contains(TEMPLATE, app):warning("Using CONFIG feature 'console' for non-app template")
CONFIG -= windows
dll {
   QMAKE_CFLAGS += $$QMAKE_CFLAGS_CONSOLE_DLL
   QMAKE_CXXFLAGS += $$QMAKE_CXXFLAGS_CONSOLE_DLL
   QMAKE_LFLAGS += $$QMAKE_LFLAGS_CONSOLE_DLL
} else {
   QMAKE_CFLAGS += $$QMAKE_CFLAGS_CONSOLE
   QMAKE_CXXFLAGS += $$QMAKE_CXXFLAGS_CONSOLE
   QMAKE_LFLAGS += $$QMAKE_LFLAGS_CONSOLE
}
