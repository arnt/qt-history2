qmake: $(OBJS) $(QOBJS)
	$(CXX) -o $@ $(OBJS) $(QOBJS) $(LFLAGS)
	rm -f /usr/home/bhughes/qt/main/bin/$@
	ln -s ../qmake/$@ /usr/home/bhughes/qt/main/bin/$@

install: qmake
	[ -d $(INSTALL_ROOT)/usr/home/bhughes/qt/main/bin ] || mkdir -p $(INSTALL_ROOT)/usr/home/bhughes/qt/main/bin
	-cp -f /usr/home/bhughes/qt/main/bin/qmake $(INSTALL_ROOT)/usr/home/bhughes/qt/main/bin
	[ -d $(INSTALL_ROOT)/usr/home/bhughes/qt/main ] || mkdir -p $(INSTALL_ROOT)/usr/home/bhughes/qt/main
	-cp -r -f /usr/home/bhughes/qt/main/mkspecs $(INSTALL_ROOT)/usr/home/bhughes/qt/main

clean::
	rm -f $(OBJS) $(QOBJS)

distclean:: clean
	rm -rf qmake .deps

