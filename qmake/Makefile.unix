#qmake code
OBJS=project.o main.o makefile.o unixmake2.o unixmake.o borland_bmake.o msvc_nmake.o msvc_dsp.o option.o winmakefile.o \
     projectgenerator.o metrowerks_xml.o

#qt code
QOBJS=qstring.o qtextstream.o qiodevice.o qglobal.o qgdict.o qcstring.o qdatastream.o qgarray.o qbuffer.o qglist.o \
	qptrcollection.o qfile.o qfile_unix.o qregexp.o qgvector.o qgcache.o qbitarray.o qdir.o \
	qfileinfo_unix.o qdir_unix.o qfileinfo.o qdatetime.o qstringlist.o qmap.o qlibrary.o

CFLAGS= $(QMAKE_CFLAGS_EXTRA) $(QMAKE_CFLAGS_DEBUG) \
	-I. -Igenerators -Igenerators/unix -Igenerators/win32 -Igenerators/mac -I@OUT_QTDIR@/include \
	-I@OUT_QTDIR@/src/tools -DQT_NO_TEXTCODEC -DQT_LITE_COMPONENT -DQT_NO_STL -I@QMAKESPEC@
LFLAGS=$(QMAKE_LFLAGS_EXTRA) @QMAKE_LIBRARIES@

qmake: $(OBJS) $(QOBJS)
	$(CXX) -o $@ $(OBJS) $(QOBJS) $(LFLAGS)
	rm -f @OUT_QTDIR@/bin/$@
	ln -s @OUT_QTDIR@/qmake/$@ @OUT_QTDIR@/bin/$@

install: qmake
	[ -d @QT_INSTALL_BINS@ ] || mkdir -p @QT_INSTALL_BINS@
	cp @OUT_QTDIR@/bin/qmake @QT_INSTALL_BINS@
	cp -r @REL_QTDIR@/mkspecs @QT_INSTALL_PREFIX@

clean::
	rm -f $(OBJS) $(QOBJS)

distclean:: clean
	rm -rf qmake .deps

