INCDIR	=	$(QTDIR)/include
CFLAGS	=	-O2
LFLAGS	=	-lqt
CC	=	gcc

####### End of automatically generated section

#
# $Source: /tmp/cvs/qt/tutorial/Makefile,v $
#

####### Directories

SUBDIRS =	t1 t2 t3 t4 t5 t6 t7 t8 t9 t10 t11 t12 t13 t14

####### Targets

all: targets

targets:
	set -e; for i in $(SUBDIRS); do ( cd $$i ; $(MAKE) 'CC=$(CC)' 'CFLAGS=$(CFLAGS)' 'LFLAGS=$(LFLAGS)' ) ; done

showdirs:
	@echo $(SUBDIRS)

showfiles:
	@echo README Makefile

depend:
	set -e; for i in $(SUBDIRS); do ( cd $$i ; $(MAKE) depend ) ; done

clean:
	set -e; for i in $(SUBDIRS); do ( cd $$i ; $(MAKE) clean ) ; done
