# lout -EPS authority.lout > qurl-authority.eps
@SysInclude { tbl }
@SysInclude { diag }
@SysInclude { picture }
@Illustration
    @InitialFont { Helvetica Base 13p }
{
@Diag
    outlinestyle {noline}
    margin {0.1c}
{
@Tbl
    #rule { yes } rulecolour { lightgreen }
    iv { ctr }
    indent { ctr }
    aformat { @Cell A |
              @StartHSpan @Cell B | @HSpan |
              @Cell width{4.6c} D | @Cell E}
    bformat { @StartHSpan @Cell A | @HSpan | @HSpan | @HSpan | @HSpan }
    cformat { @Cell A | @StartHSpan @Cell B | @HSpan | @HSpan | @HSpan }
    dformat { @Cell indent{0c} marginleft{0c} A |
              @Cell indent{0.2c} marginleft{0c} B |
              @Cell indent{0c} marginleft{0c}C |
              @Cell D |
              @Cell indent{0c} marginleft{0.25c} E}
    bfont { Courier Base 14p }
    cmarginbelow { 0.8c }
    bmarginbelow { 0.8c }
    dmarginbelow { 0.8c }
{
@Rowc B{A:: @Node authority}
@Rowb A{F:: @Node{darkblue @Colour{ftp:"//"tray:5uQQo_f"@"ftp.example.com:2021}}}
@Rowd A{S:: @Node scheme}
      B{U:: @Node{user}} C{P:: @Node password}
      D{H:: @Node host} E{T:: @Node port}
@Rowa B{I:: @Node{user info}}
}
//
darkgreen @Colour {
@VHVCurve from{A@S} to {F@NW ++{0.155,0}} radius{0.25f}
@VHVCurve from{A@S} to {F@NE --{0.01,0}} radius{0.25f}
@VHVCurve from{S@N} to {F@SW ++{0.005,0}} radius{0.25f}
@VHVCurve from{S@N} to {F@SW ++{0.08,0}} radius{0.25f}
@VHVCurve from{U@N} to {F@SW ++{0.155,0}} radius{0.25f}
@VHVCurve from{U@N} to {F@SW ++{0.25,0}} radius{0.25f}
@VHVCurve from{P@N} to {F@SW ++{0.27,0}} radius{0.25f}
@VHVCurve from{P@N} to {F@SW ++{0.435,0}} radius{0.25f}
@VHVCurve from{I@N} to {F@SW ++{0.1625,-0.3}} radius{0.25f}
@VHVCurve from{I@N} to {F@SW ++{0.4475,-0.3}} radius{0.25f}
@VHVCurve from{H@N} to {F@S ++{-0.01,0}} radius{0.25f}
@VHVCurve from{H@N} to {F@S ++{0.355,0}} radius{0.25f}
@VHVCurve from{T@N} to {F@SE --{0.105,0}} radius{0.25f}
@VHVCurve from{T@N} to {F@SE --{0.01,0}} radius{0.25f}
}
}
}
