# lout -EPS querystring.lout > querystring.eps
@SysInclude { tbl }
@SysInclude { diag }
@SysInclude { picture }
@Illustration
    @InitialFont { Helvetica Base 13p }
{
@Diag
    aoutlinestyle {noline}
    amargin {0.1c}
    afont { Courier Base 13p }
    boutlinestyle {noline}
    bmargin {0.1c}
    bfont { Helvetica Base 13p }
{
A:: @ANode{darkblue @Colour{"http://www.example.com/cgi-bin/drawgraph.cgi?type=pie&color=green"}}
//0.8c
|14c {B:: @BNode{query string}}
//
darkgreen @Colour {
@VHVCurve from{B@N} to {A@S ++{0.195,0}} radius{0.25f}
@VHVCurve from{B@N} to {A@SE --{0.005,0}} radius{0.25f}
}
}
}
