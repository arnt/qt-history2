@SysInclude{ tbl }
@SysInclude{ diag }
@SysInclude{ picture }
# lout -EPS javaiterators2.lout > javaiterators2.eps
@Illustration
    @InitialFont { Helvetica Base 24p }
    @InitialSpace { tex }
{
@Box
    linewidth { none }
@Diag
    arrowwidth { 0.5f }
    arrowlength { 0.5f }
    ahsize { 3.0f }
    avsize { 1.5f }
    bhsize { 3.0f }
    bvsize { 1.5f }
    boutlinestyle { noline }
{
A:: @ANode{A}|0c
B:: @ANode{B}|0c
C:: @ANode{C}|0c
D:: @ANode{D}
//0c
AA:: @BNode{}|0c
BB:: @BNode{}|0c
CC:: @BNode{}|0c
DD:: @BNode{}
//
red @Colour {@Arrow from{CC@SW} to {C@SW}}
blue @Colour {
@CCurveArrow from{CC@W} to {B@SW} bias{1f}
xlabeladjust{0c -1.2c} xlabel{black @Colour{prev()}}
@CurveArrow from{CC@W} to {D@SW} bias{1f}
xlabeladjust{0c -1.2c} xlabel{black @Colour{next()}}
}
}
}
