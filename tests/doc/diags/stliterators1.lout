@SysInclude{ tbl }
@SysInclude{ diag }
@SysInclude{ picture }
# lout -EPS stliterators1.lout > stliterators1.eps
@Illustration
    @InitialFont { Helvetica Base 24p }
    @InitialSpace { tex }
{
@Box
    linewidth { none }
@Diag
    arrowwidth { 0.5f }
    arrowlength { 0.5f }
    ahsize { 3.0f }
    avsize { 1.5f }
    bhsize { 3.0f }
    bvsize { 1.5f }
    boutlinestyle { noline }
    chsize { 3.0f }
    cvsize { 1.5f }
    coutlinestyle { dotted }
{
A:: @ANode{A}|0c
B:: @ANode{B}|0c
C:: @ANode{C}|0c
D:: @ANode{D}|0c
E:: @CNode{grey @Colour{end()}}
//0c
AA:: @BNode{}|0c
BB:: @BNode{}|0c
CC:: @BNode{}|0c
DD:: @BNode{}|0c
EE:: @BNode{}
//
red @Colour {
@Arrow from{AA@S} to {A@S}
@Arrow from{BB@S} to {B@S}
@Arrow from{CC@S} to {C@S}
@Arrow from{DD@S} to {D@S}
@Arrow from{EE@S} to {E@S}
}
}
}
