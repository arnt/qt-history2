@SysInclude { tbl }
@SysInclude { diag }
@SysInclude { xrgb }
@SysInclude { picture }
@Illustration
    @InitialFont { Helvetica Base 10p }
{
@Diag
    hsize { 2.25c }
    ahsize { 2.25c }
    bhsize { 2.25c }
    break { clines }
    abreak { clines }
    bbreak { clines }
    paint { @Xrgb aliceblue }
    apaint { @Xrgb cornsilk }
    aoutline { curvebox }
    bpaint { @Xrgb slategray1 }
    coutlinestyle { noline }
#    cpaint { lightyellow }
    cmargin { 0c }
    cvsize { 0.25c }
    chsize { 0.25c }
    arrowwidth { 0.75f }
    arrowlength { 0.75f }
    backarrowwidth { 0.75f }
    backarrowlength { 0.75f }
    pathwidth { medium }
    pathdashlength { 0.3f }
{
@Tbl
    #rule { yes } rulecolour {red}
    indent { ctr }
    iv { ctr }
    marginvertical { 0.3c }
    marginhorizontal { 0.75c }
    aformat { @Cell A | @Cell B | @Cell C | @Cell indent { left } D }
    bformat { @Cell A | @Cell B | @StartHSpan @Cell C | @HSpan }
    cformat { @StartVSpan @Cell A | @Cell B | @StartVSpan @Cell C | @VSpan }
    dformat { @Cell A | @Cell B | @Cell C |
	      @StartVSpan @Cell margin { 0c } paint { lightgrey } iv { foot } strut { no } D }
    eformat { @Cell A | @Cell B | @Cell C | @VSpan }
{
    @Rowb
	C { PRO:: @Node { Multi-platform .pro project file (plain text) } }
    @Rowa
	A { LUPDATE:: @ANode { lupdate } }
	B { QMAKE:: @ANode { qmake } }
    @Rowa
	A { TS:: @BNode { .ts files (XML) } }
	B { MAKEFILE:: @BNode { make file } }
    @Rowa
	A { LINGUIST:: @CurveBox paint { @Xrgb yellowgreen } { @I{Qt Linguist} } }
	B { MAKETOOL:: @CurveBox paint { @Xrgb thistle1 } { make tool } }
	C { DESIGNER:: @CurveBox paint { @Xrgb yellowgreen } { @I{Qt Designer} } }
	D { VISUALSTUDIO:: @CurveBox paint { @Xrgb green }
		{ Development Environment (Visual Studio, Emacs, etc.) } }
    @Rowa
	B { UI:: @BNode { .ui files (XML) } }
    @Rowa
	A { TSX:: @Node { .ts files (XML) } }
	B { UIC:: @ANode { uic } }
    @Rowa
	A { LRELEASE:: @ANode { lrelease } }
	B { CPP:: @BNode { C++ class
.h "&" .cpp files } }
	C { UIH:: @Node { .ui.h files } }
	D { CPPX:: @Node { C++ class
.h "&" .cpp files } }
    @Rowa
	B {}
    @Rowd
	B { MOC:: @ANode { moc } }
	D {
	    @Tbl
		#rule { yes } rulecolour { red }
		strut { no }
		break { lines }
		marginhorizontal { 0c }
		cformat { @StartHSpan @Cell indent { ctr } A | @HSpan | @HSpan }
		bformat { @StartHSpan @Cell indent { ctr } A | @HSpan | @HSpan }
		aformat { @Cell marginright { 0.5f } A |
			  @Cell B | @Cell width { 1.5c } indent { right } C }
	    {
		@Rowc A { @Underline Key }
		@Rowa
		    A { produces "&"
consumes }
		    B { PCX:: @CNode }
		    C { PCY:: @CNode }
		    @Rowa
		    A { produces }
		    B { PX:: @CNode }
		    C { PY:: @CNode }
		    @Rowa
		    A { consumes }
		    B { CX:: @CNode }
		    C { CY:: @CNode }
		    @Rowa
		    A { controls }
		    B { CCX:: @CNode }
		    C { CCY:: @CNode }
		@Rowb A { @CurveBox paint { @Xrgb yellowgreen } { Qt GUI
Application }}
		@Rowb A { @ANode { Qt command line tool } }
		@Rowb A { @CurveBox paint { @Xrgb green } { GUI
Application } }
		@Rowb A { @Node { developer file } }
		@Rowb A { @BNode { generated file } }
	    }
	}
    @Rowe
	B { MOCCPP:: @BNode { moc_*.cpp files } }
    @Rowe
	C { COMPILER:: @CurveBox paint { @Xrgb thistle1 } { compiler } }
    @Rowe
	C { OBJECTS:: @BNode { object files } }
    @Rowe
	C { LINKER:: @CurveBox paint { @Xrgb thistle1 } { linker } }
    @Rowc
	A { QM:: @BNode { .qm files
(binary) } }
	C { EXE:: @CurveBox paint { @Xrgb green }
	    { Native
executable
(Windows,
X11, Mac~OS~X) } }
}
//
@VHArrow from { LUPDATE } to { PRO } pathstyle{dotted} pathwidth{0.1f}
@VHArrow from { QMAKE } to { PRO } pathstyle{dotted} pathwidth{0.1f}
@Arrow from { LUPDATE } to { TS }
@Arrow from { TS } to { LINGUIST }
@Link from { LINGUIST } to { TSX } arrow { both } pathstyle { dashed }
@Arrow from { TSX } to { LRELEASE }
@Arrow from { LRELEASE } to { QM }
@HVHArrow from { MAKETOOL } to { LUPDATE } pathdashlength{0.6f} pathstyle{dashed}
@HVHArrow from { MAKETOOL } to { LRELEASE } pathdashlength{0.6f} pathstyle{dashed}
@LVRArrow from { MAKETOOL } to { UIC } bias{1.0f} pathdashlength{0.6f} pathstyle{dashed}
@LVRArrow from { MAKETOOL } to { MOC } bias{1.0f} pathdashlength{0.6f} pathstyle{dashed}
@LVRArrow from { MAKETOOL } to { COMPILER } bias{1.75f} pathdashlength{0.6f} pathstyle{dashed}
@LVRArrow from { MAKETOOL } to { LINKER } bias{1.75f} pathdashlength{0.6f} pathstyle{dashed}
@Arrow from { QMAKE } to { MAKEFILE }
@Arrow from { MAKETOOL } to { MAKEFILE } pathstyle{dotted} pathwidth{0.1f}
@Arrow from { UIC } to { CPP }
@Arrow from { UIC } to { UI } pathstyle{dotted} pathwidth{0.1f}
#@VHVArrow from { MOC } to { CPP@SSW } pathstyle{dotted} pathwidth{0.1f}
@Arrow from { MOC } to { CPP } pathstyle{dotted} pathwidth{0.1f}
@Arrow from { MOC } to { MOCCPP }
@VHArrow from { COMPILER } to { MOCCPP } pathstyle{dotted} pathwidth{0.1f}
@VHVLine from { PRO } to { DESIGNER } arrow { both } pathstyle { dashed }
@VHVLine from { PRO } to { VISUALSTUDIO } hfrac{0.61} arrow { both } pathstyle { dashed }
@VHArrow from { DESIGNER } to { UI }
@Arrow from { DESIGNER } to { UIH }
@Arrow from { VISUALSTUDIO } to { CPPX }
@Arrow from { COMPILER } to { UIH } pathstyle{dotted} pathwidth{0.1f}
@VHVArrow from { COMPILER } to { CPPX } hfrac{0.85} pathstyle{dotted} pathwidth{0.1f}
#@VHVArrow from { COMPILER } to { CPP@SSE } hfrac{0.85} pathstyle{dotted} pathwidth{0.1f}
@VHVArrow from { COMPILER } to { CPP } hfrac{0.85} pathstyle{dotted} pathwidth{0.1f}
@Arrow from { COMPILER } to { OBJECTS }
@Arrow from { LINKER } to { OBJECTS } pathstyle{dotted} pathwidth{0.1f}
@Arrow from { LINKER } to { EXE }
@Arrow from { EXE } to { QM }pathstyle{dotted} pathwidth{0.1f}
@Link from { PCX } to { PCY } arrow { both } pathstyle { dashed }
@Arrow from { PX } to { PY } 
@Arrow from { CY } to { CX } pathstyle{dotted} pathwidth{0.1f}
@Arrow from { CCX } to { CCY } pathdashlength{0.6f} pathstyle{dashed} 
}
}
