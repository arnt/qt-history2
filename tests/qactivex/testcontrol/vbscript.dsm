'------------------------------------------------------------------------------
'FILE DESCRIPTION: Test ActiveQt server module via VBScript
'------------------------------------------------------------------------------

Sub InitObject
	Dim A(4)
	A(0) = 5
	A(1) = "String"
	A(2) = FALSE
	A(3) = #1 Jan 94#

	With QTestControl
		.list = A
		.unicode = "Unicode"
		.text = "Text"
		.boolval = FALSE
		.number = 0
		.posNumber = 0
		.real = 0
		.color = "#123456"
		.font = "Courier New, 12"
		.date = #1 Jan 93#
		.time = "1993-01-01T13:00:00"
		.datetime = "1993-01-01T13:00:00"
		.beta = "AlphaA"
		.currency = 0

		.subType.unicode = "SubUnicode"
		.subType.number = -1
	End With
End Sub

Sub GetUnicode
	var = QTestControl.unicode
	MsgBox( var )
End Sub

Sub SetUnicode
	QTestControl.unicode = "Test Unicode"
End Sub

Sub GetText
	var = QTestControl.text
	MsgBox( var )
End Sub

Sub SetText
	QTestControl.text = "Test Text"
End Sub

Sub GetBool
	var = QTestControl.boolval
	MsgBox( var )
End Sub

Sub SetBool
	QTestControl.boolval = Not QTestControl.boolval
End Sub

Sub GetNumber
	var = QTestControl.number
	MsgBox( var )
End Sub

Sub SetNumber
	QTestControl.number = -12345
End Sub

Sub GetPosNumber
	var = QTestControl.posnumber
	MsgBox( var )
End Sub

Sub SetPosNumber
	QTestControl.posnumber = 12345
End Sub

Sub GetReal
	var = QTestControl.real
	MsgBox( var )
End Sub

Sub SetReal
	QTestControl.real = 3.1215927
End Sub

Sub SetSubUnicode
	QTestControl.subType.unicode = "SubTest Unicode"
End Sub
