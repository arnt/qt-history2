####### Compiler, tools and options

CC	=	gcc
CFLAGS	=	-Wall -W -O2 -fno-strength-reduce
INCPATH	=	-I$(QTDIR)/include
LINK	=	gcc
LFLAGS	=	
LIBS	=	-L$(QTDIR)/lib -lqt -L/usr/X11R6/lib -lX11
MOC	=	moc

####### Files

HEADERS =	2000.h
SOURCES =	2000.cpp
OBJECTS =	2000.o
SRCMOC	=	moc_2000.cpp
OBJMOC	=	moc_2000.o
TARGET	=	2000

####### Implicit rules

.SUFFIXES: .cpp .cxx .cc .C .c

.cpp.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: $(TARGET)

$(TARGET): $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

moc: $(SRCMOC)

tmake: Makefile

Makefile: 2000.pro
	tmake 2000.pro -o Makefile

clean:
	-rm -f $(OBJECTS) $(OBJMOC) $(SRCMOC) $(TARGET)
	-rm -f *~ core

####### Compile

