<?xml version="1.0"?>
<GTK-Interface>

<project>
  <name>New-sound-properties</name>
  <program_name>new-sound-properties</program_name>
  <directory></directory>
  <source_directory>src</source_directory>
  <pixmaps_directory>pixmaps</pixmaps_directory>
  <language>C</language>
