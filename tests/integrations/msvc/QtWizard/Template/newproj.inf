$$// newproj.inf = template for list of template files
$$//  format is 'sourceResName' \t 'destFileName'
$$//    The source res name may be preceded by any combination of '=', '-',  '!', '?', ':', '#', and/or '*'
$$//       '=' => the resource is binary
$$//       '-' => the file should not be added to the project (all files are added to the project by default)
$$//       '!' => the file should be marked exclude from build
$$//       '?' => the file should be treated as a help file
$$//       ':' => the file should be treated as a resource
$$//       '#' => the file should be treated as a template (implies '!')
$$//       '*' => bypass the custom AppWizard's resources when loading
$$//	if name starts with / => create new subdir

$$IF(!PROJECT_EMPTY_QT)
main.cpp	.\main.cpp
$$ENDIF

$$IF(PROJECT_QT_GUI)
app.h	.\$$Root$$App.h
app.cpp	.\$$Root$$App.cpp
window.h	.\$$Root$$Window.h
window.cpp	.\$$Root$$Window.cpp
menu.h	.\$$Root$$Menu.h
menu.cpp	.\$$Root$$Menu.cpp
toolbar.h	.\$$Root$$Toolbar.h
toolbar.cpp	.\$$Root$$Toolbar.cpp
view.h	.\$$Root$$View.h
view.cpp	.\$$Root$$View.cpp
$$ENDIF

$$IF(QT_CUSTOMWIDGET)
custom.h	.\$$Root$$Widget.h
custom.cpp	.\$$Root$$Widget.cpp
$$ENDIF

$$IF(QT_MDI)
workspace.h	.\$$Root$$Workspace.h
workspace.cpp	.\$$Root$$Workspace.cpp
:=background.png	.\background.png
$$ENDIF

$$IF(QT_TOOLBAR)
:=filenew.bmp	.\filenew.bmp
:=fileopen.bmp	.\fileopen.bmp
:=filesave.bmp	.\filesave.bmp
:=fileprint.bmp	.\fileprint.bmp
$$ENDIF
