####### Build rules

all: Makefile $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: qdoc4.pro ../../.qmake.cache $(QTDIR)/mkspecs/linux-g++/qmake.conf ../../lib/libqt.prl
	$(QMAKE) -spec /home/jasmin/qt/mkspecs/linux-g++ qdoc4.pro
qmake: 
	@$(QMAKE) -spec /home/jasmin/qt/mkspecs/linux-g++ qdoc4.pro

dist: 
	@mkdir -p .obj/debug/qdoc4 && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/debug/qdoc4/ && ( cd `dirname .obj/debug/qdoc4` && $(TAR) qdoc4.tar qdoc4 && $(GZIP) qdoc4.tar ) && mv `dirname .obj/debug/qdoc4`/qdoc4.tar.gz . && rm -rf .obj/debug/qdoc4

mocclean:
	-rm -f $(OBJMOC)
	-rm -f $(SRCMOC)

uiclean:

clean: mocclean
