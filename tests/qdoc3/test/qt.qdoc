# qt.qdoc

include( compat.qdoc )

project			= Qt
language		= C++
headerdirs		= $QTDIR/include
sourcedirs		= $QTDIR/src \
			  $QTDIR/doc
sources.fileextensions	= "*.cpp *.doc"
exampledirs		= $QTDIR/examples/advanced \
			  $QTDIR/examples/simple \
			  $QTDIR/examples \
			  $QTDIR/tutorial
imagedirs		= $QTDIR/gif
outputdir		= output/qt
base			= file:$QTDIR/doc/html/
versionsym		= QT_VERSION_STR
defines			= Q_QDOC \
			  QT_.*_SUPPORT

extraimages.HTML	= logo32

HTML.style		= "h3.fn,span.fn { margin-left: 1cm; text-indent: -1cm; }\n" \
			  "a:link { color: #004faf; text-decoration: none }\n" \
			  "a:visited { color: #672967; text-decoration: none }\n" \
			  "body { background: #ffffff; color: black; }"

