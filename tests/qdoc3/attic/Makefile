####### Build rules

all: Makefile $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: qdoc3.pro ../../.qmake.cache ../../lib/libqt.prl
	$(QMAKE) qdoc3.pro
qmake: 
	@$(QMAKE) qdoc3.pro

dist: 
	@mkdir -p .obj/release/qdoc3 && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/release/qdoc3/ && ( cd `dirname .obj/release/qdoc3` && $(TAR) qdoc3.tar qdoc3 && $(GZIP) qdoc3.tar ) && mv `dirname .obj/release/qdoc3`/qdoc3.tar.gz . && rm -rf .obj/release/qdoc3

mocclean:

uiclean:

clean:
	-rm -f $(OBJECTS) 
	-rm -f *~ core *.core


####### Sub-libraries

distclean: clean
	-rm -f $(TARGET) $(TARGET)
	

