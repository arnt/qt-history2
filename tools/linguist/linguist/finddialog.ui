  <tabstops>
    <tabstop>led</tabstop>
    <tabstop>sourceText</tabstop>
    <tabstop>translations</tabstop>
    <tabstop>comments</tabstop>
    <tabstop>matchCase</tabstop>
    <tabstop>findNxt</tabstop>
    <tabstop>cancel</tabstop>
  </tabstops>
