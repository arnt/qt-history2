                        <string>Change slot access</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>&lt;b&gt;Change the access policy of the slot&lt;/b&gt;&lt;p&gt;All slots will be created virtual and should be reimplemented in subclasses.&lt;/p&gt;</string>
                    </property>
                </widget>
                <widget class="QLabel">
                    <property name="name">
                        <cstring>labelLanguage</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;Language:</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>comboLanguage</cstring>
                    </property>
                </widget>
                <widget class="QComboBox">
                    <property name="name">
                        <cstring>comboLanguage</cstring>
                    </property>
                </widget>
