        <receiver>EditSlotsBase</receiver>
        <slot>reject()</slot>
    </connection>
    <connection>
        <sender>okButton</sender>
        <signal>clicked()</signal>
        <receiver>EditSlotsBase</receiver>
        <slot>okClicked()</slot>
    </connection>
    <connection>
        <sender>deleteSlot</sender>
        <signal>clicked()</signal>
        <receiver>EditSlotsBase</receiver>
        <slot>slotRemove()</slot>
    </connection>
    <connection>
        <sender>addSlot</sender>
        <signal>clicked()</signal>
        <receiver>EditSlotsBase</receiver>
        <slot>slotAdd()</slot>
    </connection>
    <connection>
        <sender>slotName</sender>
        <signal>textChanged(const QString&amp;)</signal>
        <receiver>EditSlotsBase</receiver>
        <slot>currentTextChanged(const QString&amp;)</slot>
    </connection>
    <connection>
        <sender>slotAccess</sender>
        <signal>activated(const QString&amp;)</signal>
        <receiver>EditSlotsBase</receiver>
        <slot>currentAccessChanged(const QString&amp;)</slot>
    </connection>
    <connection>
        <sender>slotListView</sender>
        <signal>selectionChanged(QListViewItem*)</signal>
        <receiver>EditSlotsBase</receiver>
        <slot>currentItemChanged(QListViewItem*)</slot>
    </connection>
    <connection>
        <sender>slotListView</sender>
        <signal>currentChanged(QListViewItem*)</signal>
        <receiver>EditSlotsBase</receiver>
        <slot>currentItemChanged(QListViewItem*)</slot>
    </connection>
    <connection>
        <sender>comboLanguage</sender>
        <signal>activated(const QString&amp;)</signal>
        <receiver>EditSlotsBase</receiver>
        <slot>currentLanguageChanged(const QString&amp;)</slot>
    </connection>
    <slot access="protected" language="C++">init()</slot>
    <slot access="protected" language="C++">destroy()</slot>
    <slot access="protected" language="C++">currentAccessChanged(const QString &amp;)</slot>
    <slot access="protected" language="C++">currentItemChanged(QListViewItem*)</slot>
    <slot access="protected" language="C++">currentTextChanged(const QString &amp;)</slot>
    <slot access="protected" language="C++">currentLanguageChanged(const QString &amp;)</slot>
    <slot access="protected" language="C++">slotAdd()</slot>
    <slot access="protected" language="C++">slotRemove()</slot>
    <slot access="protected" language="C++">okClicked()</slot>
</connections>
