<functions>
    <function name="beforeUpdateBook(QSqlRecord*)" >{
    QSqlQuery query( "SELECT id FROM author_view WHERE name='" + 
	ComboBoxAuthor-&gt;currentText() + "';" );
    if ( query.next() )
	buffer-&gt;setValue( "authorid", query.value( 0 ) );
}</function>
    <function name="init()" >{
    // Re-populate the ComboBox (the database may have changed) 
    QSqlQuery query( "SELECT name FROM author_view ORDER BY name;" );    
    while ( query.next() ) 
	ComboBoxAuthor-&gt;insertItem( query.value( 0 ).toString()); 
}</function>
    <function name="primeInsertBook(QSqlRecord*)" >{
    QSqlQuery query( "SELECT nextval('book_seq');" );   
    if ( query.next() )   
	buffer-&gt;setValue( "id", query.value( 0 ) );       
}</function>
    <function name="primeUpdateBook(QSqlRecord*)" >{
    // Select this book's author  
    QSqlQuery query( "SELECT name FROM author_view WHERE id=" +   
    	buffer-&gt;value( "authorid" ).toString() + ";" ); 
    QString author = "";
    if ( query.next() )
	author = query.value( 0 ).toString();    
    int i; 
    for( i = 0; i &lt; ComboBoxAuthor ) { 
	ComboBoxAuthor-&gt;insertItem( query.value( 0 ).toString(), i ); 
	if ( query.value( 0 ).toString() == author ) 
	    authorIndex = i; 
	i++; 
    } 
    // Scroll ComboBox to show the author 
    ComboBoxAuthor-&gt;setCurrentItem( authorIndex );    
}</function>
</functions>
