#############################################################################
# Makefile for building targets in sub directories.
# Generated by tmake at 17:09, 2000/07/28
#     Project: designer
#    Template: subdirs
#############################################################################

MAKEFILE=	Makefile
TMAKE	=	tmake

SUBDIRS =	util \
		uic \
		designer

all: $(SUBDIRS)

util: FORCE
	cd util
	$(MAKE)
	@cd ..

uic: FORCE
	cd uic
	$(MAKE)
	@cd ..

designer: FORCE
	cd designer
	$(MAKE)
	@cd ..


tmake: Makefile

Makefile: designer.pro
	tmake designer.pro -o Makefile

tmake_all:
	cd util
	$(TMAKE) util.pro -o $(MAKEFILE)
	@cd ..
	cd uic
	$(TMAKE) uic.pro -o $(MAKEFILE)
	@cd ..
	cd designer
	$(TMAKE) designer.pro -o $(MAKEFILE)
	@cd ..


clean:
	cd util
	$(MAKE) clean
	@cd ..
	cd uic
	$(MAKE) clean
	@cd ..
	cd designer
	$(MAKE) clean
	@cd ..

FORCE:
