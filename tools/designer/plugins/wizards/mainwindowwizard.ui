    <function name="currentMenuChanged(const QString&amp;)" >{
    
}</function>
    <function name="currentSettingChanged()" >{
    setAppropriate( menuPage, checkFileMenu-&gt;isChecked() || checkEditMenu-&gt;isChecked() || checkHelpMenu-&gt;isChecked() );
    setAppropriate( toolbarsPage, checkFileToolbar-&gt;isChecked() || checkEditToolbar-&gt;isChecked() || checkHelpToolbar-&gt;isChecked() );
    setFinishEnabled( menuPage, !appropriate( toolbarsPage ) );
    setFinishEnabled( toolbarsPage, TRUE );
    setFinishEnabled( menuToolbarPage, !appropriate( toolbarsPage ) &amp;&amp; !appropriate( menuPage ) );
}</function>
