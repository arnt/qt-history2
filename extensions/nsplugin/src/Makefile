####### Compiler, tools and options

CC	=	gcc
CFLAGS	=	-O2 -fno-strength-reduce -fPIC
INCPATH	=	-I$(QTDIR)/include
LINK	=	
LFLAGS	=	
LIBS	=	
MOC	=	moc
AR	=	ar cq
RANLIB	=	ranlib

####### Files

HEADERS =	qnp.h
SOURCES =	qnp.cpp
OBJECTS =	qnp.o
SRCMOC	=	moc_qnp.cpp
OBJMOC	=	moc_qnp.o
TARGET	=	libqnp.a
TARGETA	=	libqnp.a

####### Implicit rules

.SUFFIXES: .cpp .cxx .cc .C .c

.cpp.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all:  $(TARGET) include

staticlib: $(TARGETA) include

$(TARGETA): $(OBJECTS) $(OBJMOC)
	-rm -f $(TARGETA)
	$(AR) $(TARGETA) $(OBJECTS) $(OBJMOC)
	ranlib $(TARGETA)
	-mv $(TARGETA) ../../../lib

tmake: Makefile

Makefile: nsplugin.pro
	tmake nsplugin.pro -nodepend -o Makefile

moc: $(SRCMOC)

include:
	( D=`pwd`; cd ../../../include/ ; ln -s -f $$D/*.h . )

clean:
	-rm -f $(OBJECTS) $(OBJMOC) $(SRCMOC)
	-rm -f $(TARGETA) $(TARGETD) $(TARGET0) $(TARGET1)
	-rm -f *~ core

####### Compile

qnp.o: qnp.cpp

moc_qnp.o: moc_qnp.cpp \
		qnp.h

moc_qnp.cpp: qnp.h
	$(MOC) qnp.h -o moc_qnp.cpp

