####### Compiler, tools and options

CC	=	gcc
CFLAGS	=	-Wall -W -O2 -fno-strength-reduce -fPIC
INCPATH	=	-I$(QTDIR)/include
LINK	=	gcc
LFLAGS	=	-shared -Wl,-soname,libqnp.so
LIBS	=	-L$(QTDIR)/lib -lqt -L/usr/X11R6/lib -lX11
MOC	=	moc
AR	=	ar cq
RANLIB	=	ranlib

####### Files

HEADERS =	jri.h \
		jri_md.h \
		jritypes.h \
		npapi.h \
		npupp.h \
		qnp.h
SOURCES =	qnpbind.cpp \
		npunix.c
OBJECTS =	qnpbind.o \
		npunix.o
SRCMOC	=	moc_qnp.cpp
OBJMOC	=	moc_qnp.o
TARGET	=	qnp
TARGETA	=	libqnp.a
TARGETD	=	libqnp.so.0.1
TARGET0	=	libqnp.so
TARGET1	=	libqnp.so.0

####### Implicit rules

.SUFFIXES: .cpp .c .C

.cpp.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: sharedlib 

install: sharedlib
	cp -f -d $(TARGET0) $(TARGET1) $(TARGETD) $(QTDIR)/lib
	SDIR=$$PWD; cd $(QTDIR)/include; \
	for i in $(HEADERS); \
	do \
	    ln -f -s $$SDIR/$$i .; \
	done

sharedlib: $(TARGETD)

$(TARGETD): $(OBJECTS) $(OBJMOC)
	-rm -f $(TARGETD) $(TARGET0) $(TARGET1)
	$(LINK) $(LFLAGS) -o $(TARGETD) $(OBJECTS) $(OBJMOC) $(LIBS)
	-ln -s $(TARGETD) $(TARGET0)
	-ln -s $(TARGETD) $(TARGET1)

staticlib: $(TARGETA)

$(TARGETA): $(OBJECTS) $(OBJMOC)
	-rm -f $(TARGETA)
	$(AR) $(TARGETA) $(OBJECTS) $(OBJMOC)
	ranlib $(TARGETA)

moc: $(SRCMOC)

clean:
	-rm -f $(OBJECTS) $(OBJMOC) $(SRCMOC)
	-rm -f $(TARGETA) $(TARGETD) $(TARGET0) $(TARGET1)
	-rm -f *~ core

